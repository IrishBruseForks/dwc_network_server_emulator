{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".mypy_cache": true,
    "**/__pycache__": true
  },
  "mypy-type-checker.args": ["--config-file=./mypy.ini"],
  "[python]": {
    "editor.defaultFormatter": "eeyore.yapf"
  },
  "python.analysis.typeCheckingMode": "off",
  "explorer.excludeGitIgnore": false,
  "mypy-type-checker.importStrategy": "fromEnvironment"
}
